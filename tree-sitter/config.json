{
	"parser-directories": [
		"/home/bill/src",
		"/home/bill/.config/tree-sitter/parsers"
	],
	"theme": {
		"attribute": {
			"color": "#f9a080",
			"italic": true
		},
		"comment": {
			"color": "#c0c0c0",
			"italic": true
		},
		"constant": "#00ff00",
		"constant.builtin": {
			"bold": true,
			"color": "#f000f0"
		},
		"constructor": "#00ffbf",
		"embedded": null,
		"function": "#ff5f00",
		"function_definition": "#f0f000",
		"function_declaration": "#f0f000",
		"function.builtin": {
			"bold": true,
			"color": "#f0f000"
		},
		"keyword": "#00afff",
		"module": "#0000f0",
		"number": {
			"bold": true,
			"color": "#f0cf00"
		},
		"operator": {
			"bold": true,
			"color": "#f000f0"
		},
		"property": "#ffcf00",
		"property.builtin": {
			"bold": true,
			"color": "#ffcf00"
		},
		"punctuation": "#f000f0",
		"punctuation.bracket": "#f000f0",
		"punctuation.delimiter": "#f000f0",
		"punctuation.special": "#f000f0",
		"string": "#00ff80",
		"string.special": "#f07fff",
		"tag": "#ff0000",
		"type": "#3fefff",
		"type.builtin": {
			"bold": true,
			"color": "#ff5070"
		},
		"variable": "#00d050",
		"variable.builtin": {
			"bold": true,
			"color": "#00d050"
		},
		"variable.parameter": {
			"color": "#00d050",
			"underline": true
		}
	}
}
